<div class="modal fade" id="respoModal" tabindex="-1" role="dialog" aria-labelledby="addModalTitle" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="addModalTitle">Editar respuesta auto</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <div class="card-body">
                    <div class="form-group">
                        <label for="exampleInputName1">Key</label>
                        <input type="Name" disabled class="form-control" id="keyResponse" />
                    </div>
                    <div class="form-group">
                        <label for="dataResponse">Data</label>
                        <input type="Phone" class="form-control" id="dataResponse"
                            placeholder="Escribe el mensaje automatico que desea para esta interaccion del bot." />
                    </div>
                    <div class="container-fluid">
                        <b>
                            <p id="text-help-register"></p>
                        </b>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-dismiss="modal">
                    Cerrar
                </button>
                <button type="button" class="btn btn-primary" onclick="saveDataResponse()">Guardar cambios</button>
            </div>
        </div>
    </div>
</div>